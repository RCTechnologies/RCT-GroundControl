{
  "name": "rct-groundcontrol",
  "productName": "rct-groundcontrol",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "description": "My Electron application description",
  "repository": "https://github.com/RCTechnologies/RCT-npm GroundControl",
  "keywords": ["Drone", "Flightcontrol"],
  "author": ["Robert Elving","Christopher Mortensen"],
  "main": "src/index.ts",
  "bugs": {
    "url": "https://github.com/RCTechnologies/RCT-GroundControl/issues"
  },
  "scripts": {
    "start": "electron-forge start",
    "package:mac": "electron-forge package",
    "package:linux": "electron-forge package --platform=linux --arch=x64",
    "package:windows": "electron-forge package --platform=windows --arch=x64",
    "make": "electron-forge make",
    "lint": "tslint src",
    "test": "npm run build && ava --verbose dist/tests/*.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls ",
    "check" : "nyc check-coverage --lines 90 --functions 90 --branches 90",
    "watch": "npm-watch",
    "build": "tsc -p ."
  },
  "watch": {
    "test": "src/**/*.*",
    "start": "src/**/*.*"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": "rct_groundcontrol"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "rctgroundcontrol"
      }
    }
  },
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@types/electron": "1.4.32",
    "angular2-google-maps": "^0.17.0",
    "electron-compile": "6.1.2",
    "electron-devtools-installer": "2.1.0",
    "python-shell": "^0.4.0",
    "reflect-metadata": "0.1.10",
    "tslib": "1.6.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-preset-env": "1.1.8",
    "babel-preset-react": "6.23.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "coveralls": "2.11.16",
    "electron-forge": "2.8.0",
    "electron-packager": "8.5.2",
    "electron-prebuilt-compile": "1.4.15",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "npm-watch": "0.1.8",
    "nyc": "10.1.2",
    "protractor": "5.1.0",
    "requirejs": "2.3.3",
    "spectron": "3.6.0",
    "tslint": "4.4.2",
    "typescript": "2.1.6"
  }
}
